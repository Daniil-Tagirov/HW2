import java.util.Random();

public static void Loot() {

  public void run() {
    Random loot = new Random();
    
    public int gold = loot.nextInt();
    if (player.getPlayerClass.equals("Theif") {
      gold = gold * 1.2;
    }
    Player.onLoot(gold);
    System.out.println("You found " + gold + " gold.");
  }
}
