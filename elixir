import java.util.Random();

public static void Elixir() {

  public void run() {
    Random heal = new Random();
    
    public int getHealth(Player id);
    int health = id.getHealth;
    if (player.getClass.equals("Theif") {
      health = health*1.2;
    }
    player.onHeal(health)
    System.out.println("You gained " + health + " health");
  }
}
