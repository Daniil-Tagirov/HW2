import java.util.Random();

public class Elixir() {
  public static void Elixir(Player name) {

    public void run() {
      Random heal = new Random();

      public int getHealth(name);
      int health = heal.nextInt(12);
      name.onHeal(health)
      System.out.println("You gained " + health + " health");
    }
  }
}
